<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="traveldreams.com.br">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.TravelDreams">
        <activity
            android:name=".ui.activity.SummaryCompareActivity"
            android:exported="true"
            android:parentActivityName=".ui.activity.TravelPackageList"/>
        <activity
            android:name=".ui.activity.PaymentActivity"
            android:exported="true" />
        <activity
            android:name=".ui.activity.ResumePackageActivity"
            android:exported="true" />
        <activity
            android:name=".ui.activity.TravelPackageList"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>